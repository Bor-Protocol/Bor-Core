# OpenAI Configuration
OPENAI_API_KEY=                    # Your OpenAI API key
REDPILL_API_KEY=                   # REDPILL API key
GROQ_API_KEY=                      # Groq API key
OPENROUTER_API_KEY=                # OpenRouter API key
GOOGLE_GENERATIVE_AI_API_KEY=      # Gemini API key

# Voice Generation Settings
ELEVENLABS_XI_API_KEY=             # ElevenLabs API key
PLAYHT_API_KEY=                    # PlayHT API key
PLAYHT_USER_ID=                    # PlayHT User ID

# ElevenLabs Voice Configuration
ELEVENLABS_MODEL_ID=               # ElevenLabs model ID (default: eleven_multilingual_v2)
ELEVENLABS_VOICE_ID=               # ElevenLabs voice ID
ELEVENLABS_VOICE_STABILITY=        # Voice stability (0-1)
ELEVENLABS_VOICE_SIMILARITY_BOOST= # Voice similarity boost (0-1)
ELEVENLABS_VOICE_STYLE=            # Voice style (0-1)
ELEVENLABS_VOICE_USE_SPEAKER_BOOST=# Use speaker boost (true/false)
ELEVENLABS_OPTIMIZE_STREAMING_LATENCY= # Streaming latency optimization (0-4)
ELEVENLABS_OUTPUT_FORMAT=          # Output audio format (e.g., pcm_16000)

# BORP Server Configuration
BORP_SERVER_URL=                   # BORP server URL

# X (Twitter) Configuration
X_SERVER_URL=                      # X server URL
XAI_API_KEY=                       # X AI API key
XAI_MODEL=                         # X AI model name

# Embedding Configuration
USE_OPENAI_EMBEDDING=              # Set to TRUE to use OpenAI embeddings, leave blank for local

# OpenRouter Model Configuration
OPENROUTER_MODEL=                  # Main OpenRouter model
SMALL_OPENROUTER_MODEL=            # Model for small tasks
MEDIUM_OLLAMA_MODEL=               # Model for medium tasks
LARGE_OLLAMA_MODEL=                # Model for large tasks

# Ollama Configuration
OLLAMA_SERVER_URL=                 # Ollama server URL (default: localhost:11434)
OLLAMA_MODEL=                      # Main Ollama model
OLLAMA_EMBEDDING_MODEL=            # Embedding model (default: mxbai-embed-large)

# Ollama Task-Specific Models
SMALL_OLLAMA_MODEL=                # Model for small tasks (default: llama3.2)
MEDIUM_OLLAMA_MODEL=               # Model for medium tasks (default: hermes3)
LARGE_OLLAMA_MODEL=                # Model for large tasks (default: hermes3:70b)

# Anthropic Configuration
ANTHROPIC_API_KEY=                 # Anthropic API key for Claude

# Additional API Keys
BIRDEYE_API_KEY=                   # Birdeye API key
HELIUS_API_KEY=                    # Helius API key

# Database Configuration
# if you want to use postgres fill this otherwise will use sqlite wihtout configuration
POSTGRES_URL=                      # PostgreSQL connection URL

# Additional Configuration
TOGETHER_API_KEY=                  # Together AI API key
SERVER_PORT=                       # Server port number (default: 3000)
